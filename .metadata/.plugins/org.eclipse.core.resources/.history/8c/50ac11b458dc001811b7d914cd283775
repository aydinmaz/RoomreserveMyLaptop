package it.finsoft.RoomReserve.model;

public class Room2 {
	protected Long id;
	protected String  day;
	protected int  hour;
	protected boolean confirm;
	protected User user;
	


	public Room2() {
		super();
	}
	

	public Room2(String day, int hour, boolean confirm) {
		super();
		this.day = day;
		this.hour = hour;
		this.confirm = confirm;
	}


	public Long getId() 
	{  return id;     }
	public void setId(Long id)
	{  this.id = id;  }
	
	
	 public int getHour() 
	 {  return hour;   }
	public void setHour(int hour) 
	{   this.hour = hour;  }

	
	public String getDay() 
	{  return day;  }
	public void setDay(String day) 
	{  this.day = day; }

	
	public boolean isConfirm() 
	{  return confirm; }
    public void setConfirm(boolean confirm) 
    {   this.confirm = confirm;  }

  
	public User getUser() {
		return user;
	}


	public void setUser(User user) {
		this.user = user;
	}

   
	
}